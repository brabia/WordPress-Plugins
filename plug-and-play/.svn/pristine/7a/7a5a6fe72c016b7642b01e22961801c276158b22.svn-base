#bPress{
	background: url(../images/32.png) no-repeat scroll left center transparent;
	display: inline-block;
    float: left;
    height: 32px;
    margin-right: 10px;
    margin-top: 7px;
    width: 32px;
}

#bPress_container{
    background:#f8fafa;
}
#bPress_container .accordion-header{
	display: block;
    padding: 10px;
    font-size: 14px;
    text-align: left;
    background-color: #fff;
    border: 1px solid #ddd;
    cursor: pointer;
    border-radius: 0 3px 0 0;
	margin-top: 5px;
}
#bPress_container .accordion-header i{
	float: right;
}
#bPress_container .accordion-notification-success{
	background: #4AD5C0;
    border: 1px solid #13B7A8;
	border-radius: 3px;
	color: #fff;
	line-height: 20px;
}
#bPress_container .accordion-notification{
	border-radius: 3px;
	line-height: 20px;
}
#bPress_container .accordion-notification-success{
	background: #4AD5C0;
    border: 1px solid #13B7A8;
	color: #fff;
}
#bPress_container .accordion-notification-error{
	background: #FF6F58;
    border: 1px solid #E84D3C;
	color: #fff;
}
#bPress_container .bPress_service_content{
    display:none;
}
#bPress_container .bPress_service_content_active{
    padding: 11px 15px;
    border: 1px solid #ddd;
    border-top: 0px solid #ffba00;
    border-radius: 0  0 3px 3px;
	display: block;
}
#bPress_container .bPress_input{
    border: 1px solid #ccc;
    box-shadow: none;
    width: 100%;
    display: block;
}
#bPress_container .bPress_textarea{
    border: 1px solid #ccc;
    box-shadow: none;
    width: 100%;
    display: block;
    min-height: 250px;
}
#bPress_container .bPress_Opt{
	border: 1px dashed #ddd;
    padding: 3px;
    margin-bottom: 10px;
}
#bPress_container .bPress_radio{
    border: 1px solid #ccc;
    box-shadow: none;
    display: inline-block;
}
#bPress_container .bPress_submit{
	border: 1px solid #ccc;
    box-shadow: none;
    cursor: pointer;
}
#bPress_container .description{
	font-size: 13px;
    font-style: italic;
	margin: 2px 0 5px;
    color: #666;
	margin-top: 20px;
    padding-top: 5px;
    width: 75%;
}
#bPress_container .bPress_Config{
	margin-bottom: 10px;
}
#bPress_container ol{
	margin-left: 20px !important;
}


#bPress_container .bPressProd{
	border: 1px solid #ddd;
    height: 256px;
    width: 256px;
    overflow: hidden;
    background: #fff;
    display: inline-block;
    margin-right: 5px;
    margin-bottom: 5px;
    position: relative;
}
#bPress_container .bPressProd_prodLogo{
	text-align: center;
    height: 128px;
    width: 128px;
    margin: 0 auto;
    margin-top: 15px;
    border-radius: 70px;
}
#bPress_container .bPressProd_prodNew{
	background: url(../images/new.png);
    height: 256px;
    width: 256px;
    position: absolute;
    top: 0;
    left: 0;
    border-top: 1px solid #7BAC48;
}
#bPress_container .bPressProd:hover, #bPress_container .nProd{
	border: 1px solid #7BAC48;
}
#bPress_container .bPressProdName{
	background: #F1F1F1;
    position: absolute;
    height: 256px;
    left: 0;
    bottom: -220px;
    width: 256px;
    line-height: 30px;
    border-top: 1px solid #ddd;
    text-align: center;
    font-size: 14px;
}
#bPress_container .bPressProdName b{
	
}
#bPress_container .bPressProdBest{
	display: block;
    width: 110px;
    height: 30px;
    line-height: 25px;
    font-size: 14px;
    position: absolute;
    top: 10px;
    left: -28px;
    text-align: center;
    color: #FFF;
    background: #ff6f58;
    -ms-transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.bPress_adv{
	border: 0px;
    box-shadow: none;
}